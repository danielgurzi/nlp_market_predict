256247e90f5a55549bcdf392f688815d7c0decd.js","id":"sa-ads-api"}},"ext":[{"src":"https://www.googletagservices.com/tag/js/gpt.js","id":"gpt"},{"src":"https://js-sec.indexww.com/ht/p/183642-175291741781218.js","id":"ie"},{"src":"https://sb.scorecardresearch.com/beacon.js","id":"sb"},{"src":"https://www.googletagmanager.com/gtm.js?id=GTM-PFXR76F","id":"gtm"},{"src":"https://connect.facebook.net/en_US/sdk.js","id":"facebook-jssdk"}]},"lastRequested":"2020-07-07 16:03:29 -0400","SlugsPrices":{"disabled":false},"saSource":null,"name":null,"useSQuoteSource":"iex"},"headerConfig":{"noNotificationsMenu":null,"nonFixed":null,"tabless":null,"activeTab":null},"modules":{},"requires":[],"trackq":[],"exceptions":[],"gptInit":false};</script><script>(function(w, SA) {

  'use strict';

  var mods = SA.modules, reqs = SA.requires, log = /salog/.test(location.hash);

  function nSpace(ns, fn, force) {

    // jshint singleGroups: false

    var names = ns.split('.'), s = window.SA, l;

    if (names[0] === 'SA') {
      names = names.slice(1);
    }

    l = names.length;

    names.forEach(function(name, i) {
      if (s[name] === undefined || force) {
        s[name] = (i === l - 1 && fn && typeof fn === 'function') ? fn() : {};
      }
      s = s[name];
    });

    return s;
  }

  function clearResolved(deps) {

    // jshint singleGroups: false

    var l = [];
    deps = (deps && Array.isArray(deps)) ? deps : [];
    deps.forEach(function(d) {
      if(!mods[d] || !mods[d].resolved) {
        l.push(d);
      }
    });
    return l;
  }

  function scope(deps) {
    var res = [];
    deps.forEach(function(d) {
      res.push(nSpace(d));
    });
    return res;
  }

  function reqExec(f, deps, n) {
    SA.run(function() {
      if(typeof f === 'function') {
        SA.log('RESOLVED: ' + n);
        return f.apply(SA, scope(deps));
      }
    });
  }

  function delayRes(f, deps, n) {
    SA.log('WAITING RESOLUTION ' + n);
    return reqs.push([f, deps, n]);
  }

  function resolveReq() {
    var req = [];
    reqs.forEach(function(r){
      if(!clearResolved(r[1]).length) {
        reqExec(r[0], r[1], r[2]);
      } else {
        req.push(r);
      }
    });
    reqs = req;
  }

  function resolve(name, deps, fn) {
    SA.log('DEFINE: ' + name + '. Deps: ' + deps);
    var depsLeft = clearResolved(deps);
    mods[name] = {resolved: !depsLeft.length};
    if(mods[name].resolved) {
      SA.run(function() {
        nSpace(name, function() {
          return fn.apply(window.SA, scope(deps));
        });
        SA.log('RESOLVED: ' + name);
        resolveReq();
      });
      Object.keys(mods).forEach(function(k) {
        if(!mods[k].resolved && k !== name) {
          resolve(k, mods[k].deps, mods[k].fn);
        }
      });
    } else {
      SA.log('WAITING RESOLUTION ' + name);
      mods[name].deps = deps;
      mods[name].fn = fn;
    }
  }


  // exception handler. Executes provided function and sends custom message to Rollbar in case of exception
  // @param {function} f - function to execute
  // @param {string} m - message to send in case of execution failure (optional)
  // Usage:
  //   var div = SA.run(function() {
  //     SA.Author.show();
  //   }, 'author scope not defined')

  SA.run = function(f, m) {
    var res;
    try {
      res = f();
    } catch(e) {
      w.console.error(e);
      SA.exceptions.push([m || e.message || 'SA execution error', e, {}]);
    }
    return res;
  };

  // exception handler. Executes provided function and silently catches exception in case there was one
  // @param {function} f - function to execute
  // Usage:
  //   SA.exec(function() {
  //     SA.Author.show();
  //   })

  SA.exec = function(f) {
    var res;
    try {
      res = f();
    } catch(ignore) {}
    return res;
  };

  SA.execIfResolved = function(deps, f) {
    if(!clearResolved(deps).length) {
      reqExec(f, deps, 'EXEC-IF: ' + f.toString());
    }
  };

  // a convenience function for parsing string namespaces and
  // automatically generating nested namespaces
  // sample usage:
  // var scope = SA.UsingNamespace('SA.Widget.Test');
  // @param {string} ns - dot separated desired string namespace.

  // @return {Object} - nested object namespace.
  SA.UsingNamespace = function(ns, fn) {
    return nSpace(ns, fn);
  };

  SA.log = function(m) {
    if(log) {
      w.console.warn('%c SA-LOG: ', 'background: #1a9afd; color: white', m) ;
    }
  };

  SA.define = function(name, deps, fn) {
    if(!mods[name]) {
      resolve(name, deps, fn);
    }
  };

  SA.forceResolve = function(name, fn) {
    nSpace(name, fn);
    mods[name] = {
      resolved: true
    };
  };

  SA.require = function(deps, f, n) {
    SA.log('REQUIRE: ' + n + '. Deps: ' + deps);
    return clearResolved(deps).length ? delayRes(f, deps, n) : reqExec(f, deps, n);
  };

}(window, window.SA));

SA.define('SA.Fn.param', [], function() {

  'use strict';

  return function(name, url) {
    var regex, results;
    url = url || window.location.href;
    name = name.replace(/[\[\]]/g, '\\$&');
    regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)');
    results = regex.exec(url);
    if (!results) {
      return null;
    }
    if (!results[2]) {
      return '';
    }
    return decodeURIComponent(results[2].replace(/\+/g, ' '));
  };

});
SA.define('SA.Fn.ajax', [], function () {

  'use strict';

  return function (url, clb, opt) {
    var req = new XMLHttpRequest();
    opt = opt || {};
    opt.fail = opt.fail || function () { return false; };
    clb = clb || function () {};

    req.open(opt.method || 'GET', url, true);
    req.setRequestHeader('X-REQUESTED-WITH', 'XMLHttpRequest');
    req.onreadystatechange = function () {
      if (this.readyState === 4) {
        return (this.status >= 200 && this.status < 400) ? clb(opt.txt ? this.responseText : JSON.parse(this.responseText)) : opt.fail();
      }
    };
    req.send();
    req = null;
  };
});
SA.require([], function() {

  'use strict';

  var w = window;

  w.SA.App.initTime = Date.now();
  w.SA.App.initHref = w.location.href;
  w.SA.App.moneSource = function() {
    var m = /\/.+?(?=(\/\d+)|\/*$)/.exec(w.location.pathname) || [''];
    return m[0] || w.location.pathname;
  };
}, 'app-conf');
// Page key generation. Used in Mone and ads
SA.define('SA.pk', [], function() {

  'use strict';

  var pk;

  function gen() {
    pk = Date.now().toString(36) + Math.floor(Math.random() * 1000000000).toString(36);
    return pk;
  }

  gen();

  return {
    generate: function() {
      return gen();
    },
    get: function() {
      return pk;
    },
    set: function(newPk) {
      pk = newPk;
    }
  };

});
SA.define('SA.mark', [], function() {

  'use strict';

  return function(m) {
    window.SA.exec(function() {

      window.performance.mark(m);
      window.SA.log('MARK: ' + m);

    });
  };

});
SA.require(['SA.mark'], function(m) {

  'use strict';

  m('footer-inline-init');

}, 'footer-init-mark');
SA.define('SA.loadJS', [], function() {

  'use strict';

  var d = document, h = d.getElementsByTagName('head')[0];

  function ready(url) {
    return function () {
      var rs = this.readyState;
      if (rs && rs !== 'complete' && rs !== 'loaded') {
        return;
      }
      url.loaded = true;
      SA.log('LOADED: ' + url.src);
      if(url.callback) {
        url.callback();
      }
    };
  }

  function er(url) {
    return function() {
      SA.log('LOAD-ERROR: ' + url.src);
      if(url.error) {
        url.error();
      }
    };
  }

  return function(urls) {
    var s, gptHeaderTest;
    urls.forEach(function(url) {
      gptHeaderTest = url.id === 'gpt' && SA.App.gptHeaderTest;
      if(url.src && !gptHeaderTest) {
        s = d.createElement('script');
        s.async = true;
        s.type = 'text/javascript';
        s.src = url.src.replace('{{rnd}}', Math.floor(Math.random() * 99999999999));
        s.onload = ready(url);
        s.onreadystatechange = ready(url);
        s.onerror = er(url);
        if (url.id) {
          s.id = url.id;
        }
        h.appendChild(s);
        SA.log('REQUEST: ' + url.src);
      }
    });
  };

});
SA.define('SA.inlineCSS', [], function() {

  'use strict';

  var d = document, h = d.getElementsByTagName('head')[0], m = {};

  return function(css, mod) {
    var s;
    if(!m[mod] && css) {
      s = d.createElement('style');
      s.textContent = css;
      s.setAttribute('sa-mod', mod);
      m[mod] = s;
      h.appendChild(s);
    }
  };

});
SA.define('SA.readCookie', [], function() {

  'use strict';

  var d = document;

  return function(n) {

    return SA.exec(function() {
      var c = d.cookie.match(new RegExp('(^|;)\\s*' + escape(n) + '=([^;\\s]*)'));
      return c ? unescape(c[2]) : null;
    });

  };

});
SA.define('SA.readLS', [], function() {

  'use strict';

  var d = document;

  return function(k) {

    return SA.exec(function() {
      return window.localStorage[k] || '';
    });

  };

});
SA.define('SA.Krux', ['SA.loadJS', 'SA.readCookie'], function(js, rc) {

  'use strict';

  //PUBLIC FUNCTIONS

  return {

    reload: function() {
      if(!rc('show_desktop')) {
        var m, src = (m = location.href.match(/\bkxsrc=([^&]+)/)) && decodeURIComponent(m[1]);
        src = /^https?:\/\/([a-z0-9_\-\.]+\.)?krxd\.net(:\d{1,5})?\//i.test(src) ? src : src === 'disable' ? '' : 'https://cdn.krxd.net/controltag?confid=Jubkx3MY';
        js([{src: src}]);
      }
    }

  };

});
SA.require(['SA.readCookie', 'SA.Fn.param', 'SA.AdsConf'], function (rc, param, adsConf) {

  'use strict';

  var w = window, d = document, s = SA.pageConfig.saSource || param('source') || rc('_sasource'), e;

  function isOTM() {
    return (SA.pageConfig && SA.pageConfig.Data &&
      SA.pageConfig.Data.mc && SA.pageConfig.Data.mc.tags &&
      SA.pageConfig.Data.mc.tags.indexOf('on-the-move') > -1
    );
  }

  function isSsga() {
    return location.href.match(/\/market-outlook\/gold-and-precious-metals/);
  }

  function fromEvtImpact() {
    return /\/event-impact\/?$/.test(document.referrer);
  }

  function isNuveenCef() {
    return location.href.match(/\/etfs-and-funds\/closed-end-funds/);
  }

  function isIshares() {
    return (location.href.match(/\/etfs-and-funds\/etf-screener/) || (SA.pageConfig &&
      SA.pageConfig.Data &&
      SA.pageConfig.Data.isIshares));
  }

  function bb() {
      var notBranded = !isOTM() && !isSsga() && !isNuveenCef() && !isIshares() && !fromEvtImpact(),
      isAdsEnvironment = adsConf.isUSA || (/^test\./).test(w.location.host),
      notAdsFree = !(SA.UserCookie || {}).ad_free_user,
      inDebugMode = !!param('krux_bb'),
      fromMobileWeb = !!rc('show_desktop');
    if (((isAdsEnvironment && notAdsFree) || inDebugMode) && notBranded && !fromMobileWeb) {
      e = d.querySelectorAll('#krux-container .' + (s === 'refreshed' ? 'nonrefresh' : 'refresh'))[0];
      if (e) {
        e.parentNode.removeChild(e);
      }
      e = document.getElementById('krux-container');
      if (e) {
        e.style.display = '';
      }
    } else {
      e = document.getElementById('krux-container');
      if (e) {
        e.parentNode.removeChild(e);
      }
    }
  }

  // Adjust broker buttons
  bb();

}, 'adjust-bb');
SA.require(['SA.readCookie', 'SA.UserCookie'], function(rc, uc) {

  'use strict';

  var w = window, k = uc.ads_kvs, v;

  w.SA.krux_kvs = {};

  if (rc('user_id')) {
    v = k ? [['regvoc', k.regvoc], ['interest', k.interest], ['tf', k.tf], ['nwslet', k.nwslet], ['pbkr', k.pbkr], ['accred', k.accred], ['mv', k.mv], ['app_ud', k.app_ud]] : [['reg', 't']];
    if (SA.App.pro) {
      v.push(['pp', 'sa_pro']);
    }
    v.forEach(function(kv) {
      if(kv[1]) {
        w.SA.krux_kvs[kv[0]] = kv[1];
      }
    });
  }


}, 'krux-kvs');
SA.require(['SA.Krux', 'SA.readCookie', 'SA.readLS'], function(kx, rc, ls) {

  'use strict';

  var w = window;

  function read(k) {
    return ls(k) || rc(k) || '';
  }

  w.Krux || ((w.Krux = function () {Krux.q.push(arguments)}).q = []);

  kx.reload();

  // Krux Segment

  w.Krux.user = read('kxuser');
  w.Krux.segments = (read('kxsegs') && read('kxsegs').split(',')) || [];

}, 'krux-loader');
SA.require([], function() {

  'use strict';

  var w = window;

  // Comscore setup
  w._comscore = w._comscore || [];
  w._comscore.push({c1: '2', c2: SA.App.comscoreAccountId.toString()});

  // Facebook setup
  w.fbAsyncInit = function () {
    FB.init({
      appId: w.SA.App.fbAppId,
      xfbml: true,
      version: 'v3.0'
    });
  };

}, '3rd-party-init');
SA.require(['SA.readCookie', 'SA.loadJS'], function(rc, js) {

  'use strict';

  var w = window, d = document;

  w.ucFallback = function() {
    SA.define('SA.UserCookie', [], function() {
      return {};
    });
  };

  w.user_cookie = w.user_cookie || {};

  if(rc('user_id')) {
    d.write('<scr' + 'ipt src="/account/cookie/' + rc('user_cookie_key') + '" onerror="window.ucFallback()"></scri' + 'pt>');
  } else {
    w.ucFallback();
  }

  js(SA.pageConfig.Paths.ext);

}, 'sa-inline-init');
SA.require(['SA.readCookie', 'SA.loadJS'], function(rc, js) {

  'use strict';

  var w = window, ua = navigator.userAgent || '';

  var platform = navigator.platform || '', isOSX = /Mac/.test(platform);

  var isEdge = /Edge/i.test(ua), isChrome = /chrome|crios|crmo/i.test(ua), isSafari = /safari/i.test(ua) && !isChrome && !isEdge;
  var supportedByMoat = !(isSafari && isOSX);
  var forceMoat = /force-moat/.test(window.location.href);
  var noMoat = /no-moat/.test(window.location.href) || SA.App.gptHeaderTest;

  if((supportedByMoat || forceMoat) && !noMoat) {
    js([{src: 'https://z.moatads.com/seekingalphaheader780944224702/moatheader.js', id: 'moat'}]);
  } else {
    w.skipMoat = true;
  }

}, 'sa-inline-init');
SA.define('SA.modLoader', ['SA.inlineCSS'], function(css) {

  'use strict';

  return {
    init: function(inline, fn, n) {
      if(inline) {
        css(inline, n);
      }
      if(fn) {
        fn();
      }
    }
  };

});



















