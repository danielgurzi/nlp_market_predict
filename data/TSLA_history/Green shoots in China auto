SA.define('SA.pk', [], function() {

  'use strict';

  var pk;

  function gen() {
    pk = Date.now().toString(36) + Math.floor(Math.random() * 1000000000).toString(36);
    return pk;
  }

  gen();

  return {
    generate: function() {
      return gen();
    },
    get: function() {
      return pk;
    },
    set: function(newPk) {
      pk = newPk;
    }
  };

});
